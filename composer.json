{
    "name": "topthink/think",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        },
        {
            "name": "yunwuxin",
            "email": "448901948@qq.com"
        }        
    ],
    "require": {
        "php": ">=7.2.5",
        "topthink/framework": "^6.0.0",
        "topthink/think-orm": "^2.0",
        "topthink/think-multi-app": "^1.0",
        "topthink/think-view": "^1.0",
        "topthink/think-captcha": "^3.0",
        "topthink/think-image": "^1.0",
        "nette/php-generator": "^3.4",
        "doctrine/annotations": "^1.13",
        "thans/tp-jwt-auth": "^1.2",
        "topthink/think-filesystem": "^2.0",
        "dragonmantank/cron-expression": "^3.3",
        "symfony/process": "^5.4",
        "w7corp/easywechat": "^5.0",
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "topthink/think-trace":"^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
            "woo\\": "woo"
        },
        "psr-0": {
            "": "extend/"
        },
        "files": [
            "woo/common/helper/helper.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ],
        "post-install-cmd": [
            "@php -r \"if (strtoupper(substr(PHP_OS, 0, 3)) === 'WIN') { passthru('powershell -ExecutionPolicy Bypass -File scripts/fix-jwt-php8.ps1'); } else { passthru('bash scripts/fix-jwt-php8.sh'); }\""
        ],
        "post-update-cmd": [
            "@php -r \"if (strtoupper(substr(PHP_OS, 0, 3)) === 'WIN') { passthru('powershell -ExecutionPolicy Bypass -File scripts/fix-jwt-php8.ps1'); } else { passthru('bash scripts/fix-jwt-php8.sh'); }\""
        ]
    }
}
